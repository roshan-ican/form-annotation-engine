      "position": { "x": 36, "y": 636, "width": 381, "height": 14 },
      "binding": { "path": "taxpayer.address.line1" },
      "nativeFieldId": "topmostSubform[0].Page1[0].Address_ReadOrder[0].f1_20[0]"
    },
    {
      "id": "address_apt",
      "label": "Apt. no.",
      "page": 1,
      "type": "text",
      "position": { "x": 419, "y": 636, "width": 65, "height": 14 },
      "binding": { "path": "taxpayer.address.aptNo" },
      "nativeFieldId": "topmostSubform[0].Page1[0].Address_ReadOrder[0].f1_21[0]"
    },
    {
      "id": "address_city",
      "label": "City, town, or post office",
      "page": 1,
      "type": "text",
      "position": { "x": 36, "y": 612, "width": 294, "height": 14 },
      "binding": { "path": "taxpayer.address.city" },
      "nativeFieldId": "topmostSubform[0].Page1[0].Address_ReadOrder[0].f1_22[0]"
    },
    {
      "id": "address_state",
      "label": "State",
      "page": 1,
      "type": "text",
      "position": { "x": 332, "y": 612, "width": 63, "height": 14 },
      "binding": { "path": "taxpayer.address.state" },
      "nativeFieldId": "topmostSubform[0].Page1[0].Address_ReadOrder[0].f1_23[0]"
    },
    {
