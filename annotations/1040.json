{
  "form": {
    "id": "irs-1040",
    "name": "U.S. Individual Income Tax Return",
    "year": 2025,
    "pages": 2,
    "pageSize": { "width": 612, "height": 792, "unit": "pt" }
  },
  "fields": [
    {
      "id": "taxpayer_first_name",
      "label": "Your first name and middle initial",
