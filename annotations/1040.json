      "type": "text",
      "position": { "x": 36, "y": 684, "width": 215, "height": 14 },
      "binding": { 
        "path": "taxpayer.name.first",
        "transform": "uppercase"
      },
      "format": { "fontSize": 10, "align": "left" },
      "nativeFieldId": "topmostSubform[0].Page1[0].f1_14[0]"
    },
    {
      "id": "taxpayer_last_name",
      "label": "Last name",
      "page": 1,
      "type": "text",
      "position": { "x": 253, "y": 684, "width": 214, "height": 14 },
      "binding": { "path": "taxpayer.name.last" },
      "nativeFieldId": "topmostSubform[0].Page1[0].f1_15[0]"
    },
    {
      "id": "taxpayer_ssn",
      "label": "Your social security number",
      "page": 1,
      "type": "ssn",
      "position": { "x": 469, "y": 684, "width": 107, "height": 14 },
      "binding": { "path": "taxpayer.ssn" },
      "nativeFieldId": "topmostSubform[0].Page1[0].f1_16[0]"
