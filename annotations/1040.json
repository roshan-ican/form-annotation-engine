      "label": "ZIP code",
      "page": 1,
      "type": "text",
      "position": { "x": 397, "y": 612, "width": 70, "height": 14 },
      "binding": { "path": "taxpayer.address.zip" },
      "nativeFieldId": "topmostSubform[0].Page1[0].Address_ReadOrder[0].f1_24[0]"
    },
    {
      "id": "filing_status_single",
      "label": "Filing Status: Single",
      "page": 1,
      "type": "checkbox",
